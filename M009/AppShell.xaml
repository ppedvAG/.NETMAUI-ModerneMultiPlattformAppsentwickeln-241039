<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="M009.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:M009"
    Shell.FlyoutBehavior="Disabled"
    Title="M009">

	<!--Shell Navigation-->
	<!--Einzelne Pages per ShellContent definieren-->
	<!--Jeder ShellContent benötigt eine Route um mit GoToAsync diese ansteuern zu können-->
	<!--<TabBar>-->
		<ShellContent
			Title="Home"
			ContentTemplate="{DataTemplate local:MainPage}"
			Route="MainPage"/>
		<ShellContent Title="Page2"
					  ContentTemplate="{DataTemplate local:SecondaryPage}"
					  Route="Page2"/>
	<!--</TabBar>-->

</Shell>
